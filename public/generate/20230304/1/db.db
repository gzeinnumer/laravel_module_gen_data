CREATE TABLE examples (
id INTEGER PRIMARY KEY NOT NULL AUTOINCREMENT, 
name TEXT NOT NULL, 
flag_active INTEGER NOT NULL, 
created_at TEXT NOT NULL, 
updated_at TEXT DEFAULT NULL
);

